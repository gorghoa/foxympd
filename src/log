define([
    'jquery',
    'underscore',
    'backbone',

    'dbs',

    'backbone.indexeddb'

],function($,_,Backbone,dbs) {


    /**
     * Represent an mpd connection settings 
     */
    var Model = Backbone.Model.extend({

        database:dbs['default'],
        storeName:"connections",
        getDisplayName:function() {

            if(this.get('name')) {
                return this.get('name');
            } else {
             return this.get('host') +':'+ this.get('port');
            }

        }

    });


    return Model;



});
