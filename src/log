define([
    'jquery',
    'underscore',
    'backbone',

    //plugins
    'backbone.subroute'

], function($,_,Backbone) {

    var router = Backbone.SubRoute.extend({

        appManager:null,        

        routes: {

            "": "list", 
            "build": "build",
            "save": "save"
        },

        build : function() {
            var self=this;
            require(['views/playlists/build'], function (View) {
                var view = new View();
                self.appManager.showView(view);
              });  
        },

        save : function() {
            var self=this;
            require(['views/playlists/save'], function (View) {
                var view = new View();
                self.appManager.showView(view);
              });  
        },

        list : function() {
            var self=this;
            require(['views/mpdconnections/list'], function (View) {
                var view = new View();
                self.appManager.showView(view);
              });  
        }

    });


    return router;


});


