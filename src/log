define([
    'jquery',
    'underscore',
    'backbone',

    'dbs',
    'models/mpdconnection',

    'backbone.indexeddb'



],function($,_,Backbone,dbs,MPDConnectionModel) {


    var collection= Backbone.Collection.extend({

        database:dbs['default'],
        storeName:"connections",
        model:MPDConnectionModel

    });


    return collection;
});

